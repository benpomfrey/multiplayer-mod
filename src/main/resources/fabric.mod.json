{
	"schemaVersion": 1,
	"id": "multiplayermod",
	"version": "${version}",
	"name": "Multiplayer Mod",
	"description": "A simple reverse proxy mod that allows LAN servers to be joined over WAN",
	"authors": [
		"bebguin"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/multiplayermod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.example.OpenInit"
		],
		"client": [
			"com.example.InitTrack"
		]
	},
	"mixins": [
		"multiplayermod.mixins.json",
		{
			"config": "modid.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}